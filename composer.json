{
    "name": "prof-code/lagen",
    "description": "Lagen",
    "keywords": ["lagen"],
    "license": "MIT",
    "type": "package",
    "require": {
        "php": ">=7.0",
        "atnic/eloquent-filters": "^2.2",
        "laravel/framework": "^8.12",
        "laravel/passport": ">=4.0",
        "propaganistas/laravel-intl": ">=2.1",
        "propaganistas/laravel-phone": "^4.2"
    },
    "require-dev": {
        "fideloper/proxy": "~3.3",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/tinker": "~1.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "prof-code\\lagen\\": "app/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "prof-code\\lagen\\Providers\\AppServiceProvider",
                "prof-code\\lagen\\Providers\\ConsoleServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
